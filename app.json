{
  "expo": {
    "name": "Moovelogic",
    "slug": "moove-app",
    "scheme": "moovelogic",
    "platforms": [
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.oyekunmi.moovelogic",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.oyekunmi.moovelogic"
    },
    "entryPoint": "node_modules/expo/AppEntry.js",
    "description": ""
  }
}
